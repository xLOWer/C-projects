#include <stdio.h>

int main(void){
	int b[3][3], e = 0, w = 0; bool l = false;
	for(int i = 0; i < 3; ++i)
	{
		for(int j = 0; j < 3; ++j)
		{
			scanf("%d", &m[i][j]);
		}
	}
	x = 0; y = x;
	printf("\n");
	for(int i = 0; i < 3; ++i){
		for(int j = 0; j < 3; ++j){
			if(i == j)
			{
				x += m[i][j];
			}
			
			if(i + j == 2)
			{
				y += m[i][j];
			}
		}
		printf("\n");
	}
	for(int i = 0; i < 3; ++i){
		if(((m[i][0] + m[i][1] + m[i][2])== x && (m[0][i] + m[1][i] + m[2][i])== x && (x)== x && (y)== x)){
			k = true;}else{k = false;}
	}
	if(k == true){printf("MAGICAL\n"); } else{ printf("NOT MAGICAL\n");}
	printf("glavnaya:\t%d\n", y);
	printf("poboxhnaya:\t%d\n\n", x);
}
